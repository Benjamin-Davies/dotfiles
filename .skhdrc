# Comma ','
cmd + alt - 0x2B : open -a System\ Preferences
# Dot '.'
cmd + alt - 0x2F : open -a Calculator
# Enter
cmd + alt - return : open -a Terminal

cmd + alt - a : open -a Calendar
cmd + alt - b : open -a Firefox
cmd + alt - g : open -a GarageBand
cmd + alt - i : open -a Finder
cmd + alt - m : open -a Mail
cmd + alt - s : open -a Spotify
cmd + alt - u : open -a Musescore\ 3
cmd + alt - v : open -a Visual\ Studio\ Code
